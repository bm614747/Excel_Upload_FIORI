    <Page id="page" title="{i18n>title}">
        <content>
            <core:Fragment fragmentName="excelupload.fragments.commonUploadUI" type="XML" />
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer />
                <Button text="{i18n>DOWNLOAD_TEMPLATE_TXT}" type="accept"/>
            </Toolbar>
        </footer>
    </Page>
</mvc:View>
